{
  "name": "glauth-keycloak",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "containerEnv": {
    "KUBECONFIG": "/workspace/kubeconfig"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "ms-kubernetes-tools.vscode-kubernetes-tools"
      ]
    }
  },
  "forwardPorts": [8080, 8081],
  "postCreateCommand": ".devcontainer/postCreateCommand.sh"
}
